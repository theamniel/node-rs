{
  "name": "@amniel/humanize",
  "version": "0.1.0",
  "description": "duration and bytes to human-readable (private use)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "Apache-2.0",
  "author": "Amniel",
  "exports": {
    ".": "./lib/index.js",
    "./esm": "./lib/esm.mjs"
  },
  "files": [
    "./lib/index.js",
    "./lib/esm.mjs",
    "./lib/index.d.ts",
    "./lib/bindings.js"
  ],
  "scripts": {
    "artifacts": "napi artifacts -d ../../artifacts",
    "build": "napi build --platform --release --js bindings.js lib",
    "build:debug": "napi build --platform --js bindings.js lib",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheAmniel/node-rs.git"
  },
  "napi": {
    "name": "humanize",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.3"
  }
}